{
    "project": {
        "projectName": "FuckScaffold",
        "js": {
            "src": "client/js/src",
            "local": "static/js",
            "build": "static/js"
        },
        "css": {
            "src": "client/sass",
            "local": "static/css",
            "build": "static/css"
        }
    },
    "hash": [
        {
            "dist": {
                "js": "server/config/version_js.json",
                "css": "server/config/version_css.json"
            },
            "template": {
                "head": "{",
                "record": "\"<%= prefix %><%= name %>\": \"<%= path %>\"",
                "foot": "}"
            }
        }
    ],
    "cdn": {
        "dist": "/FuckScaffold/build"
    },
    "build": {
        "extra": {
            "shims": "",
            "paths": "",
            "commonModule": "superman.common.js",
            "appendEntry": true,
            "esVersion": 7
        },
        "confPath": ""
    }
}
